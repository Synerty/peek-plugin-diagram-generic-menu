{
    "plugin": {
        "title": "Diagram Generic Menu",
        "packageName": "peek_plugin_generic_diagram_menu",
    "version":"1.2.2",
        "buildNumber": "#PLUGIN_BUILD#",
        "buildDate": "#BUILD_DATE#",
        "creator": "Synerty Pty Ltd",
        "website": "www.synerty.com"
    },
    "requiresServices": [
        "server",
        "storage",
        "admin",
        "client",
        "mobile",
        "desktop"
    ],
    "server": {
    },
    "storage": {
        "alembicDir": "_private/alembic"
    },
    "admin": {
        "showHomeLink": true,
        "appDir": "_private/admin-app",
        "appModule": "genericDiagramMenu.module#GenericDiagramMenuModule",
        "moduleDir": "plugin-module"
    },
    "client": {
    },
    "mobile": {
        "moduleDir": "plugin-module",
        "rootServices": [
            {
                "file": "_private/services/PrivateGenericTupleService",
                "class": "PrivateGenericTupleService"
            },
            {
                "file": "_private/services/PrivateGenericMenuService",
                "class": "PrivateGenericMenuService",
                "persistent": true
            }
        ]
    },
    "desktop": {
        "moduleDir": "plugin-module",
        "rootServices": [
            {
                "file": "_private/services/PrivateGenericTupleService",
                "class": "PrivateGenericTupleService"
            },
            {
                "file": "_private/services/PrivateGenericMenuService",
                "class": "PrivateGenericMenuService",
                "persistent": true
            }
        ]
    }
}
